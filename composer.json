{
    "name": "kpicaza/sudoku",
    "description": "Sudoku game program",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kpicaza\\Sudoku\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Kpicaza\\Sudoku\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "symfony/runtime": "^5.3",
        "twig/twig": "^3.0",
        "symfony/framework-bundle": "^6.2",
        "symfony/twig-bundle": "^6.2",
        "symfony/webpack-encore-bundle": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^5.4",
        "vimeo/psalm": "^5.9",
        "phpro/grumphp": "^1.15",
        "symplify/easy-coding-standard": "^11.3",
        "rector/rector": "^0.15.24",
        "antidot-fw/dev-tools": "^0.1.1",
        "filp/whoops": "^2.5",
        "franzl/whoops-middleware": "^2.0",
        "laminas/laminas-component-installer": "^3.2",
        "phpstan/phpstan": "^1.2",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "infection/extension-installer": true,
            "laminas/laminas-component-installer": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "check-all": [
            "@cs-check",
            "@test",
            "@static",
            "@suggestions"
        ],
        "cs-check": "ecs check config src tests",
        "cs-fix": "ecs check config src tests --fix",
        "suggestions": "rector process src --dry-run",
        "rector": "rector process src",
        "static": "psalm",
        "test": "phpunit"
    }
}
